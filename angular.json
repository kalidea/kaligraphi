{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "playground": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "hammerjs"
            ],
            "outputPath": "dist/playground",
            "stylePreprocessorOptions": {
              "includePaths": [
                "./src",
                "./dist/kalidea/kaligraphi/styles"
              ]
            },
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.sass"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "playground:build",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "playground:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "@kalidea/kaligraphi": {
      "root": "projects/kalidea/kaligraphi",
      "sourceRoot": "projects/kalidea/kaligraphi/src",
      "projectType": "library",
      "prefix": "kal",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/kalidea/kaligraphi/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kalidea/kaligraphi/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/kalidea/kaligraphi/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kalidea/kaligraphi/src/test.ts",
            "tsConfig": "projects/kalidea/kaligraphi/tsconfig.spec.json",
            "karmaConfig": "projects/kalidea/kaligraphi/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/kalidea/kaligraphi/**/*.ts",
              "projects/kalidea/kaligraphi/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@kalidea/kaligraphi",
  "schematics": {
    "@schematics/angular:component": {
      "viewEncapsulation": "None",
      "style": "sass",
      "changeDetection": "OnPush"
    },
    "@schematics/angular:module": {
      "export": true
    }
  },
  "cli": {
    "analytics": "3932d92d-82cf-4dfe-9914-ad99ab89761d",
    "defaultCollection": "@angular-eslint/schematics"
  }
}
