{
  "name": "kaligraphi",
  "version": "11.0.0",
  "scripts": {
    "dev": "ng build @kalidea/kaligraphi --watch",
    "serve": "ng serve --open",
    "test": "ng test @kalidea/kaligraphi",
    "ng": "ng",
    "build": "ng build --configuration production",
    "build:lib": "npm run build",
    "copy:styles": "cpx \"./projects/kalidea/kaligraphi/src/**/*.{sass,scss,css}\" ./dist/kalidea/kaligraphi",
    "compile:styles": "node-sass ./projects/kalidea/kaligraphi/src/styles/styles.sass ./dist/kalidea/kaligraphi/styles/styles.css",
    "postbuild": "npm run copy:styles && npm run compile:styles",
    "test:ci": "ng test @kalidea/kaligraphi --karma-config=./projects/kalidea/kaligraphi/karma.test.conf.js --code-coverage",
    "lint": "ng lint @kalidea/kaligraphi",
    "serve:playground": "npm run serve",
    "build:playground": "ng build playground --configuration production",
    "build:all": "ng build --configuration production && npm run build:playground",
    "verify": "ng lint @kalidea/kaligraphi && npm run test:ci && npm run build:all",
    "verify:windows": "ng lint @kalidea/kaligraphi && npm run build:all && npm run test",
    "prepublish-next": "npm run build; cp README.md dist/kalidea/kaligraphi/;cd dist/kalidea/kaligraphi/",
    "publish-next": "npm publish --access public --tag next"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.14",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.15",
    "luxon": "^1.25.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular-devkit/core": "^12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.5",
    "node-sass": "^4.9.4",
    "protractor": "~7.0.0",
    "scss-bundle": "^2.4.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "tslint-jasmine-rules": "^1.5.1",
    "typescript": "~4.3.5"
  }
}
