{
  "name": "kaligraphi",
  "version": "13.0.0",
  "scripts": {
    "dev": "ng build @kalidea/kaligraphi --watch",
    "serve": "ng serve --open",
    "test": "ng test @kalidea/kaligraphi",
    "ng": "ng",
    "build": "ng build --configuration production",
    "build:lib": "npm run build",
    "compile:styles": "sass ./projects/kalidea/kaligraphi/src/styles/styles.sass ./dist/kalidea/kaligraphi/styles/styles.css",
    "postbuild": "npm run compile:styles",
    "test:ci": "ng test @kalidea/kaligraphi --karma-config=./projects/kalidea/kaligraphi/karma.test.conf.js",
    "lint": "ng lint @kalidea/kaligraphi",
    "serve:playground": "npm run serve",
    "build:playground": "ng build playground --configuration production",
    "build:all": "ng build --configuration production && npm run build:playground",
    "verify": "ng lint @kalidea/kaligraphi && npm run test:ci && npm run build:all",
    "verify:windows": "ng lint @kalidea/kaligraphi && npm run build:all && npm run test",
    "prepublish-next": "npm run build; cp README.md dist/kalidea/kaligraphi/;cd dist/kalidea/kaligraphi/",
    "publish-next": "npm publish --access public --tag next"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.1",
    "@angular/cdk": "~13.0.0",
    "@angular/common": "~13.0.1",
    "@angular/compiler": "~13.0.1",
    "@angular/core": "~13.0.1",
    "@angular/forms": "~13.0.1",
    "@angular/platform-browser": "~13.0.1",
    "@angular/platform-browser-dynamic": "~13.0.1",
    "@angular/router": "~13.0.1",
    "codelyzer": "^6.0.2",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.15",
    "luxon": "^1.25.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "~13.0.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.3",
    "tslint": "~6.1.0",
    "tslint-jasmine-rules": "^1.5.1",
    "typescript": "~4.4.4"
  }
}
